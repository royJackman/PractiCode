<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:PractiCode.Views.JavaScript;assembly=PractiCode"
            xmlns:controls="clr-namespace:PractiCode.Controls"
            x:Class="PractiCode.Views.JavaScript.JavaScriptMain"
            Title="JavaScript"
            BarBackgroundColor="{StaticResource JavaScriptPrimary}"
            BarTextColor="{StaticResource JavaScriptText}">
    <NavigationPage.TitleView>
        <controls:LanguageNavbarView LanguageNavbarName="JavaScript"
                                     LanguageNavbarVersion="v1.8.5"
                                     LanguageNavbarTextColor="{StaticResource JavaScriptText}"
                                     LanguageNavbarImage="javascript.png"/>
    </NavigationPage.TitleView>
    <local:JavaScriptInterpreter Title="Interpreter"/>
    <local:JavaScriptExamples Title="Examples"/>
</TabbedPage>